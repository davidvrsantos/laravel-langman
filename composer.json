{
    "name": "davidvrsantos/laravel-langman",
    "description": "Manage language files with ease. Forked to support laravel 7",
    "keywords": ["laravel", "localization", "multilingual"],
    "homepage": "https://github.com/themsaid/laravel-langman",
    "license": "MIT",
    "authors": [
        {
            "name": "Smarter Vision",
            "email": "smartersvision@gmail.com"
        },
        {
            "name": "Mohamed Said",
            "email": "theMohamedSaid@gmail.com",
            "homepage": "https://themsaid.github.io"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "9.52.16",
        "illuminate/console": "9.52.16",
        "illuminate/filesystem": "9.52.16"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4.8 || ^5.0 || ^6.0 || ^7.0 || ^8.0",
        "orchestra/testbench": "~3.0",
        "orchestra/database": "~3.0",
        "mockery/mockery": "^0.9.4 || ^1.0"
    },
    "autoload": {
        "psr-4": {
            "Themsaid\\Langman\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Themsaid\\Langman\\LangmanServiceProvider"
            ]
        }
    }
}
